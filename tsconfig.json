{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "module": "es2015",
    "lib": ["ES2015", "DOM"],
    "noImplicitUseStrict": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "declaration": true,
    "sourceMap": true
  }
,
"exclude": [ "dist"]
}
